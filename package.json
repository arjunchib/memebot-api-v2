    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^15.0.0",
    "mongodb": "^3.5.7",
    "validator": "^13.7.0",
    "ytdl-core": "^2.1.2"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
