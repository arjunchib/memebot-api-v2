{
  "name": "memebot-api",
  "version": "1.0.0",
  "description": "The API for memebot",
  "main": "src/index.js",
  "repository": "https://github.com/arjunchib/memebot-api.git",
  "author": "Arjun Chib <arjunpatrickchib@gmail.com>",
